{
    "name": "redux-sacala",
    "version": "0.3.7",
    "description": "A library for creating composable Redux blocks with state, actions, and effects",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/monkin/redux-sacala.git"
    },
    "bugs": {
        "url": "https://github.com/monkin/redux-sacala/issues"
    },
    "homepage": "https://github.com/monkin/redux-sacala#readme",
    "type": "module",
    "keywords": [
        "redux",
        "state-management",
        "composition",
        "typescript",
        "toolkit"
    ],
    "author": {
        "name": "Andrey Monkin",
        "email": "monkin.andrey@gmail.com"
    },
    "files": [
        "build"
    ],
    "sideEffects": false,
    "engines": {
        "node": ">=18"
    },
    "module": "build/redux-sacala.js",
    "main": "build/redux-sacala.cjs",
    "types": "build/redux-sacala.d.ts",
    "exports": {
        ".": {
            "types": "./build/redux-sacala.d.ts",
            "import": "./build/redux-sacala.js",
            "require": "./build/redux-sacala.cjs"
        }
    },
    "peerDependencies": {
        "@reduxjs/toolkit": "^2.0.0",
        "redux": "^5.0.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@reduxjs/toolkit": "2.11.2",
        "@types/node": "25.0.9",
        "@typescript-eslint/eslint-plugin": "8.53.0",
        "@typescript-eslint/parser": "8.53.0",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "prettier": "3.8.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.53.0",
        "vite": "7.3.1",
        "vite-plugin-dts": "4.5.4",
        "vitest": "4.0.17"
    },
    "scripts": {
        "build": "vite build",
        "prepublishOnly": "npm run lint && npm test && npm run build",
        "test": "vitest run",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write ."
    }
}
